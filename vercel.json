{
    "rewrites": [
      {
        "source": "/assets/:path*",
        "destination": "/assets/:path*"
      },
      {
        "source": "/:path*",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "/assets/:path*.js",
        "headers": [
          { "key": "Cache-Control", "value": "no-cache" }
        ]
      }
    ]
  }
  